	justify-content: center;
	font-size: 25px;
	color: white;
	font-weight: bold;
	background-color: #2F54F2;
	margin-top: 10px;
	margin-right: 10%;
	padding: 0;
}

.point_text{
	max-width: 100%;
	max-height: 60%;
}

.documents_image{
	width: 50%;
	min-height: 100%;
	background-color: #FFF5E6;
	display: flex;
	justify-content: center;
	align-items: center;
}

.documents_image > img{
	width: 80%;
}

@media (max-width: 900px) {
	.documents_presentation{
		width: 100%;
	}

	.documents_image{
